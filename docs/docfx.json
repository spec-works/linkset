{
  "metadata": [
    {
      "src": [
        {
          "files": ["Linkset/Linkset.csproj"],
          "src": "../dotnet/src"
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["**.md", "**.yml"],
        "exclude": ["obj/**", "_site/**", "**/bin/**", "**/obj/**"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "template": ["default", "modern"],
    "globalMetadata": {
      "_appTitle": "Linkset Documentation",
      "_appName": "Linkset",
      "_appFooter": "<a href='https://spec-works.github.io/'>SpecWorks Factory</a> | <a href='https://github.com/spec-works/linkset'>GitHub</a>",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_gitContribute": {
        "repo": "https://github.com/spec-works/linkset",
        "branch": "main"
      }
    }
  }
}
